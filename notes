CREATE TABLE user ( id INT NOT NULL AUTO_INCREMENT, first_name VARCHAR(50) NOT NULL, last_name VARCHAR(50) NOT NULL, email VARCHAR(255) NOT NULL, PRIMARY KEY (id) );

SELECT * FROM user;